  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.18",
    "bcrypt": "^5.1.0",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb-memory-server": "^9.0.0",
    "mongoose": "^7.3.2",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.7.2",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
